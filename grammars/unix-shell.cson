            'include': '$self'
          }
        ]
      }
      {
        'begin': '(?<=\\s|^)(\\{)(?=\\s|$)'
        'captures':
          '1':
            'name': 'punctuation.definition.group.shell'
        'end': '(?<=^|;)\\s*(\\})'
        'name': 'meta.scope.group.shell'
        'patterns': [
          {
            'include': '$self'
          }
        ]
      }
    ]
  'function-definition':
    'patterns': [
