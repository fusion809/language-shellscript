          '3':
            'name': 'punctuation.definition.arguments.shell'
        'end': '(\\||;|&|$)'
        'endCaptures':
          '0':
            'name': 'punctuation.definition.function.shell'
        'name': 'meta.function.shell'
        'patterns': [
          {
            'include': '$self'
